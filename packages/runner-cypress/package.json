{
  "name": "@uuv/cypress",
  "version": "1.2.0-beta-20230424",
  "type": "commonjs",
  "author": "Louis Fredice NJAKO MOLOM (https://gitlab.com/curacao) & Stanley SERVICAL (https://gitlab.com/stanleyservical)",
  "description": "A transpiler from cucumber step definitions to cypress.",
  "license": "Apache-2.0",
  "homepage": "https://e2e-test-quest.github.io/uuv/",
  "repository": {
    "type": "git",
    "url": "https://github.com/e2e-test-quest/uuv"
  },
  "bin": {
    "uuv": "./dist/lib/uuv-cli.js"
  },
  "keywords": [ "uuv", "UUV" ,"E2E", "test", "testing", "cypress", "testing-library", "cucumber", "gherkin"],
  "scripts": {
    "generate:step-definitions": "ts-node generate-step-definitions.ts",
    "generate:step-definitions-documentation": "ts-node generate-step-definitions-documentation.ts",
    "package": "cp ../../LICENSE ./dist && cp ../../CONTRIBUTING.md ./dist && cp ../../DCO.txt ./dist && npm pack --pack-destination=\"../../dist/packages\"",
    "postinstall": "node postinstall.js",
    "test:run": "node test.js --run",
    "test:open": "node test.js --open"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "16.0.3",
    "@cypress/webpack-preprocessor": "5.17.0",
    "@testing-library/cypress": "9.0.0",
    "@uuv/runner-commons": "1.2.0-beta-20230424",
    "axe-core": "4.7.0",
    "chalk": "4.1.2",
    "cucumber-json-report-formatter": "0.1.4",
    "cypress": "12.11.0",
    "cypress-axe": "1.4.0",
    "figlet": "1.6.0",
    "is-admin": "2.1.1",
    "minimist": "1.2.8",
    "multiple-cucumber-html-reporter": "3.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist/cucumber/**/*",
    "dist/cypress/**/*",
    "dist/lib/**/*",
    "dist/index.d.ts",
    "dist/index.js",
    "postinstall.js",
    "target-config/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cypress/commands": {
      "import": "./dist/cypress/commands.js",
      "require": "./dist/cypress/commands.js",
      "types": "./dist/cypress/commands.d.ts"
    },
    "./cypress/cypress.config": {
      "import": "./dist/cypress/cypress.config.js",
      "require": "./dist/cypress/cypress.config.js",
      "types": "./dist/cypress/cypress.config.d.ts"
    },
    "./cucumber/step_definitions": {
      "import": "./dist/cucumber/step_definitions/**/*.js",
      "require": "./dist/cucumber/step_definitions/**/*.js",
      "types": "./dist/cucumber/step_definitions/**/*.d.ts"
    }
  },
  "devDependencies": {
    "@types/minimist": "1.2.2",
    "@types/node": "18.15.11",
    "cross-env": "7.0.3",
    "serve-handler": "6.1.5",
    "ts-loader": "9.4.2",
    "tsconfig-paths": "4.2.0",
    "webpack": "5.76.1"
  }
}
