{
  "name": "@uuv/assistant",
  "version": "1.2.0-beta-20230424",
  "author": "Louis Fredice NJAKO MOLOM (https://gitlab.com/curacao) & Stanley SERVICAL (https://gitlab.com/stanleyservical)",
  "description": "UUV wizard used to improve the life of testers and developers by generating cucumber phrases from the GUI.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/e2e-test-quest/uuv"
  },
  "homepage": "https://e2e-test-quest.github.io/uuv/",
  "keywords": [
    "uuv",
    "UUV",
    "E2E",
    "test",
    "testing",
    "cypress",
    "testing-library",
    "cucumber",
    "playwright",
    "gherkin"
  ],
  "bin": {
    "uuv-assistant": "./dist/launcher/uuv-assistant-launcher.js"
  },
  "scripts": {
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "launch": "ts-node launcher/uuv-assistant-launcher.ts",
    "launch:custom": "ts-node tests/uuv-assistant-custom-launcher.ts",
    "conf:build": "node compute-conf.js",
    "package": "cp ../../LICENSE ./dist && cp ../../CONTRIBUTING.md ./dist && cp ../../DCO.txt ./dist && npm pack --pack-destination=\"../../dist/packages\""
  },
  "dependencies": {
    "@cucumber/cucumber": "9.1.0",
    "playwright-chromium": "1.32.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/jquery": "^3.5.16",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "bootstrap": "^5.2.3",
    "deepmerge": "^4.3.1",
    "dom-accessibility-api": "^0.5.16",
    "grommet-icons": "^4.10.0",
    "inspector-dom": "^0.1.1",
    "minimist": "1.2.8",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "replace-json-property": "1.8.0",
    "styled-components": "^5.3.9",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "files": [
    "dist/launcher/**/*",
    "build/static/css/**/*.css",
    "build/static/js/main.*.js",
    "postinstall.js",
    "target-config/**/*"
  ]
}
